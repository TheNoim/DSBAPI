{
  "name": "dsbapi",
  "dependencies": {
    "axios": "^0.16.2",
    "pako": "^1.0.4",
    "percentage-calc": "^0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2017": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "joi": "^10.6.0",
    "jsdoc": "^3.5.4",
    "jsome": "^2.3.26",
    "minami": "^1.2.3",
    "prepack": "^0.2.6",
    "uglify-js": "^3.0.28"
  },
  "version": "2.1.0",
  "description": "An api for mobile.dsbcontrol.de",
  "author": "Nils Bergmann <nilsbergmann@noim.io>",
  "main": "index.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheNoim/DSBAPI.git"
  },
  "keywords": [
    "dsb",
    "api",
    "mobile"
  ],
  "license": "MIT",
  "scripts": {
    "generate-doc": "jsdoc --configure ./.jsdoc.json --verbose index.js",
    "build-browser": "browserify index.js --standalone DSB -t [ babelify --presets [ es2015 es2017 ] --plugins [ add-module-exports transform-runtime ] ] > dist/index.browser.js",
    "transpile": "babel index.js --presets es2015 --plugins add-module-exports > index.es.js",
    "minify": "uglifyjs dist/index.browser.js -o dist/index.browser.min.js",
    "prepack": "prepack dist/index.browser.js --out dist/index.prepack.js",
    "build": "yarn run transpile && yarn run build-browser && yarn run minify"
  }
}
