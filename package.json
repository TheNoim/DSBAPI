{
  "name": "dsbapi",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "pako": "^1.0.4",
    "percentage-calc": "^0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "chai": "^4.1.2",
    "joi": "^13.7.0",
    "jsdoc": "^3.5.4",
    "jsome": "^2.3.26",
    "minami": "^1.2.3",
    "mocha": "^5.2.0",
    "prepack": "^0.2.6",
    "uglify-js": "^3.0.28"
  },
  "version": "3.0.2",
  "description": "An api for mobile.dsbcontrol.de",
  "author": "Nils Bergmann <nilsbergmann@noim.io>",
  "main": "index.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheNoim/DSBAPI.git"
  },
  "keywords": [
    "dsb",
    "api",
    "mobile"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "generate-doc": "jsdoc --configure ./.jsdoc.json --verbose index.js",
    "build-browser": "browserify index.js --standalone DSB -t [ babelify --presets [ env ] --plugins [ add-module-exports transform-runtime ] ] > dist/index.browser.js",
    "transpile": "babel index.js --presets env --plugins add-module-exports,transform-runtime > index.es.js",
    "minify": "uglifyjs dist/index.browser.js -o dist/index.browser.min.js",
    "fprepack": "prepack dist/index.browser.js --out dist/index.prepack.js",
    "build": "yarn run transpile && yarn run build-browser && yarn run minify"
  }
}
