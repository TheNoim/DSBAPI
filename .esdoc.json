{
	"source": "./src",
	"destination": "./docs",
	"plugins": [
		{
			"name": "esdoc-standard-plugin",
			"option": {
				"accessor": { "access": ["public"], "autoPrivate": true }
			}
		},
		{
			"name": "esdoc-external-nodejs-plugin",
			"option": { "enable": true }
		},
		{
			"name": "esdoc-external-ecmascript-plugin",
			"option": { "enable": true }
		}
	]
}
